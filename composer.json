
{
    "name": "devfactory/macros",
    "description": "Laravel package",
    "keywords": ["macros", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "DevFactory",
            "email": "developer@devfactory.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravelcollective/html":"^5.4.0",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Devfactory\\Macros\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devfactory\\Macros\\MacrosServiceProvider"
            ]
        }
    }
}